  },
  "dependencies": {
    "@mdx-js/loader": "3.0.0",
    "@mdx-js/react": "3.0.0",
    "@next/mdx": "14.0.1",
    "@typethings/tailwind-config": "workspace:*",
    "@typethings/ui": "workspace:*",
    "contentlayer": "0.3.4",
    "lucide-react": "0.292.0",
    "next": "14.0.1",
    "next-contentlayer": "0.3.4",
    "next-themes": "0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype-autolink-headings": "7.1.0",
    "rehype-slug": "6.0.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1.7.2",
    "@types/mdx": "2.0.9",
    "@types/node": "20.8.10",
    "@types/react": "18.2.35",
    "@types/react-dom": "18.2.14",
    "autoprefixer": "10.4.16",
    "eslint": "8.53.0",
    "eslint-config-next": "14.0.1",
