{
  "name": "@typethings/ui",
  "author": "@pheralb_",
  "description": "Typethings UI library built with React, Tailwind CSS and shadcn/ui",
  "version": "0.1.0",
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "dev-styles": "tailwindcss -i ./src/styles/globals.css -o ./dist/index.css --watch",
    "build-styles": "tailwindcss -i ./src/styles/globals.css -o ./dist/index.css --minify",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-collapsible": "1.0.3",
    "@radix-ui/react-context-menu": "2.1.5",
    "@radix-ui/react-dialog": "1.0.5",
    "@radix-ui/react-dropdown-menu": "2.0.6",
    "@radix-ui/react-popover": "1.0.7",
    "@radix-ui/react-radio-group": "1.1.3",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-tabs": "1.0.4",
    "@radix-ui/react-tooltip": "1.0.7",
    "class-variance-authority": "0.7.0",
    "clsx": "2.0.0",
    "cmdk": "0.2.0",
    "lucide-react": "0.284.0",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "1.14.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "0.5.9",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "autoprefixer": "10.4.16",
    "eslint": "8.51.0",
    "eslint-plugin-react": "7.33.2",
    "postcss": "^8.4.31",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "1.0.7",
    "tsup": "7.2.0",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}
