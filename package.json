{
  "name": "typethings",
  "author": "@pheralb_",
  "description": "A beautiful, minimal, and fast text editor built with Tauri.",
  "private": true,
  "packageManager": "npm@8.5.0",
  "workspaces": [
    "app"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@turbo/gen": "1.9.7",
    "eslint": "7.32.0",
    "eslint-config-custom": "*",
    "prettier": "2.5.1",
    "turbo": "latest"
  }
}
